<?xml version="1.0" encoding="UTF-8"?>
<IPPhoneInputScreen
        type="combox"
        defaultIndex="1"
        style="none"
        editable="yes"
        Timeout="0"
        LockIn="no"
        LocalData="yes"
        InputType="1aB">
    <Title wrap="yes" showlistcount="yes">
        Intercom
    </Title>

		<Prompt>Active:</Prompt>
		<DataID>DID_Features_INTERCOM_Enable</DataID>
		<Default>0</Default>
		<option text="Disable">0</option>
		<option text="Enable">1</option>
	    <InputField
					type = "combox"
					password = "no"
					editable = "yes">
			<Prompt>Mute:</Prompt>
			<DataID>DID_Features_INTERCOM_Mute</DataID>
			<option text="Disable">0</option>
			<option text="Enable">1</option>
			<Default>0</Default>
	    </InputField>
</IPPhoneInputScreen>
